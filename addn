int *input()
{	int a[20]; int i;
	 printf("how many numbers you want?\n");
        scanf("%d",&i);
	printf("type the numbers\n");
	for(int x=0;x<i;x++)
	{  scanf("%d",&a[x]);
	}
        return a;
}

int compute(int a[], int i)
{	int s=0;
	for(int r=0;r<i;r++)
{
	printf("%d",a[i]);}	


for(int r=0;r<i;r++)
{
	s= s + a[i];
}
}

int output(int m, int c)
{
	printf(" the sum of  %d is %d",c,m);
}
