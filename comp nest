#include<stdio.h>
int main()
{   int x,y,z,a;
    input(&x,&y,&z);
    a = compute(x,y,z);
    output(a);
}

int input(int *a, int *b, int *c)
{
   printf("enter 1st number\n");
   scanf("%d",a);
   printf("enter 2nd number\n");
   scanf("%d",b);
   printf("enter 3nd number\n");
   scanf("%d",c);
}
// using nested if
int compute(int a, int b, int c)
{
    if(a < b)
	{if(a<c)
	 {return a;}}
    if(b<c)
	{if(b<a)
	 {return b;}}
    if(c<a)
	{if(c<b)
	 {return c;}}	
	 
}
int output(int m)
{
    printf(" the smallest number is %d", m);
}
