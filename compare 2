#include<stdio.h>


int main()
{   int x,y,z;
    input(&x,&y);
    z=compute(x,y);
    output(z);
}

int input(int *a, int*b)
{
        printf("Enter 2 numbers\n");
        scanf("%d",a);
        scanf("%d",b);


}
int compute(int a,int b)
{
    if(a < b)
    {   return a;
    }
    else
    {
        return b;
    }
}

void output(int m)
{
    printf(" the smaller number is %d", m);
}

